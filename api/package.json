    "express-async-error": "^0.0.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.0",
    "node-ts": "^5.1.2",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.4",
    "swagger-ui-express": "^4.6.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
